<?php

/**
 * @file
 * Install file for Everyday Tools Menu module.
 */

/**
 * Implements hook_install().
 */
function everyday_tools_menu_install() {
  // Create variable.
  variable_set('everyday_tools_menu_cache_time', 1800);
}

/**
 * Implements hook_install().
 */
function everyday_tools_menu_uninstall() {
  // Delete variable.
  variable_del('everyday_tools_menu_cache_time');
}